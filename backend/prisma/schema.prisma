generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Indicator {
  id                  Int  @id @default(autoincrement())
  theme               String
  subtheme            String
  indicator           String
  description         String
  entity              String
  period              Int
  value               String
  unit                String
  supporting_doc_link String?
  comment             String?
}
